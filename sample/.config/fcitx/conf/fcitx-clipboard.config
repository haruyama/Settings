[Clipboard]
# クリップボードの履歴をファイルに保存
# Available Value:
# True False
SaveHistoryToFile=False
# 保存するクリップボード履歴の数
#HistoryLength=5
# 各候補の最大文字数
#CandidateMaxLength=43
# クリップボード履歴リストのトリガーキー
TriggerKey=CTRL_J
# 選択したテキストを使用する
# Available Value:
# True False
#UsePrimary=True
# 修飾キーを選択
# Available Value:
# None
# Alt
# Ctrl
# Shift
#ChooseModifier=None
# 空白の選択を無視
# Available Value:
# True False
#IgnoreBlank=True

